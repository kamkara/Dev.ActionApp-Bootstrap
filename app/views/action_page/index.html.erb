
<% @ActionProjets.each do |action| %>
    <div class="m-2">
        <div class="my-2">
            <h1 class="font-bold text-xl">
                <%= action.title %>
            </h1>
        </div>

        <div class="card bg-gray-200 border rounded-md">
            <div class="m-2">
                <%= action.content %>
            </div>
        </div>
    </div>
<% end %>
